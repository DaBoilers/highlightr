{
    "name": "Highlightr",
    "description": "Highlight important shit",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "128": "128.png"
    },
    "options_page": "options.html"
  }